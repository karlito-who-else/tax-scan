    <key>WatchPaths</key>
    <array>
        <string>/Users/<USER>>/Library/Mobile Documents/com~apple~CloudDocs/Affairs/HMRC/Tax Return</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/<USER>>/Sites/tax-scan</string>
